{
  "expo": {
    "name": "GalleryApp",
    "slug": "GalleryApp",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/bigIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bigIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.parkergiven.galleryApp"
    },
    "androidStatusBar": {
      "barStyle": "dark-content"
      
    },
    "androidNavigationBar": {
      "barStyle": "dark-content"
    }
  }
}
